class Solution {
public:
    string intToRoman(int num) {
        string ans;
        while(num>=1000){
            ans.push_back('M');
            num-=1000;
        }
        while(num>=500){
            if(num>=900){
                ans.push_back('C');
                ans.push_back('M');
                num-=900;
            }
            else{
                 ans.push_back('D');
                num-=500;
            }
        }
        while(num>=100){
            if(num>=400){
                ans.push_back('C');
                ans.push_back('D');
                num-=400;
            }
            else{
                ans.push_back('C');
                num-=100;
            }
        }
        while(num>=50){
            if(num>=90){
                ans.push_back('X');
                ans.push_back('C');
                num-=90;
            }
            else{
                ans.push_back('L');
                num-=50;
            }
        }
        while(num>=10){
            if(num>=40){
                ans.push_back('X');
                ans.push_back('L');
                num-=40;
            }
            else{
                ans.push_back('X');
                num-=10;
            }
        }
        while(num>=5){
            if(num>=9){
                ans.push_back('I');
                ans.push_back('X');
                num-=9;
            }
            else{
                ans.push_back('V');
                num-=5;
            }
        }
        while(num>=1){
            if(num>=4){
                ans.push_back('I');
                ans.push_back('V');
                num-=4;
            }
            else{
                ans.push_back('I');
                num-=1;
            }
        }
        return ans;
    }
};
